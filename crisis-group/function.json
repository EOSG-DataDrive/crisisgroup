{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */3 * * * *",
      "useMonitor": false
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "crisisgroup",
      "path": "crisis-group/data.csv",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
